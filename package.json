  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5001",
    "vbuild": "vite build",
    "vserve": "vite preview --host 0.0.0.0 --port 5000",
    "serve": "vite serve",
    "generate-whatsnew": "node build-tools/generate-whats-new.js && git add . && git commit -m 'build: whats new'",
    "generate-zip": "node build-tools/generate-zip.js",
    "commit": "git cz",
    "pre:run": "node build-tools/move-resources.js && node build-tools/bundle-vendors.js",
    "autobuild": "rollup -c -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest  --collect-coverage",
    "todo": "node build-tools/get-todo.js",
    "cypress": "cypress open",
    "release:patch": "release-it patch --no-npm.publish",
    "release:pre": "release-it pre --no-npm.publish",
    "release:minor": "release-it minor --no-npm.publish",
    "vtest": "vitest",
    "vtest:coverage": "vitest run --coverage"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
